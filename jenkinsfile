
pipeline {
    agent any

    stages {
       stage('Buiding') {
            steps {
                echo 'Building.....'
                sh 'cd /var/lib/jenkins/workspace/JavaWebCalculator/JavaWebCalculator'
                sh 'mvn clean package'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing.....'
                sh '/usr/bin/mvn test'
            }
        }
        stage('Delopy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
