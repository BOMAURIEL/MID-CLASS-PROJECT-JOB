
pipeline {
    agent any

    stages {
        stage('Initialize') {
            steps {
                echo 'Initializing.....'
                sh '/var/lib/jenkins/workspace/JavaWebCalculator/JavaWebCalculator/mvn initialize'
            }
        }
        stage('Buiding') {
            steps {
                echo 'Building.....'
                sh '/var/lib/jenkins/workspace/JavaWebCalculator/JavaWebCalculator/mvn clean package'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing.....'
                sh '/usr/share/maven/bin/mvn test'
            }
        }
        stage('Delopy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
