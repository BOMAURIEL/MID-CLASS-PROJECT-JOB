
pipeline {
    agent any

    stages {
       stage('Buiding') {
            steps {
                echo 'Building.....'
                sh '/usr/share/maven/lib/guice.jar'
                sh '/usr/bin/mvn clean package'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing.....'
                sh '/usr/bin/mvn test'
            }
        }
        stage('Delopy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
